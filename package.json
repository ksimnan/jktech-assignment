{
    "name": "nestjs-sequelize-app",
    "version": "1.0.0",
    "description": "A NestJS application using Sequelize without SequelizeModule.forFeature",
    "main": "dist/main.js",
    "scripts": {
      "start": "node dist/main.js",
      "start:dev": "nest start --watch",
      "build": "nest build",
      "lint": "eslint '{src,apps,libs,test}/**/*.ts' --fix",
      "test": "jest",
      "test:watch": "jest --watch",
      "test:cov": "jest --coverage",
      "format": "prettier --write \"src/**/*.ts\""
    },
    "dependencies": {
      "@nestjs/common": "^10.0.0",
      "@nestjs/core": "^10.0.0",
      "sequelize": "^6.31.0",
      "sequelize-typescript": "^2.1.4",
      "pg": "^8.11.0",
      "reflect-metadata": "^0.1.13"
    },
    "devDependencies": {
      "@nestjs/cli": "^10.0.0",
      "@nestjs/testing": "^10.0.0",
      "@types/jest": "^29.0.0",
      "jest": "^29.0.0",
      "ts-node": "^10.0.0",
      "tsconfig-paths": "^4.0.0",
      "typescript": "^4.0.0"
    }
  }
  